{
    "name": "yasuaki640/php-rendering-engine-net",
    "description": "A module for network operations.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Yasuaki640\\PhpRenderingEngine\\Net\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yasuaki640\\PhpRenderingEngine\\Net\\Tests\\": "tests/"
        }
    },
    "require": {
        "yasuaki640/php-rendering-engine-core": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "scripts": {
        "test": "phpunit",
        "test-verbose": "phpunit --display-warnings --display-deprecations",
        "cs-fix": "PHP_CS_FIXER_IGNORE_ENV=1 ./vendor/bin/php-cs-fixer fix",
        "cs-check": "PHP_CS_FIXER_IGNORE_ENV=1 ./vendor/bin/php-cs-fixer fix --dry-run --diff"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        }
    ]
}
